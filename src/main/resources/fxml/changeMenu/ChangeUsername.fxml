<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.group62.veiw.changeMenu.ChangeUsernameController"
            prefHeight="800.0" prefWidth="1200.0">
    <center>
        <VBox alignment="CENTER" spacing="10">
            <TextField promptText="new username" maxWidth="300" fx:id="newUsername"/>
            <TextField text="" maxWidth="300" fx:id="text"/>
            <Button text="change username" onMouseClicked="#usernameChange"/>
            <Button text="back" onMouseClicked="#back"/>
        </VBox>
    </center>
</BorderPane>
