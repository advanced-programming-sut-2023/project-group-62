<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.group62.veiw.changeMenu.ChangePasswordController"
            prefHeight="800.0" prefWidth="1200.0">
    <center>
        <VBox alignment="CENTER" spacing="10">
            <PasswordField promptText="old password" maxWidth="300" fx:id="oldPassword"/>
            <PasswordField promptText="new password" maxWidth="300" fx:id="newPassword"/>
            <TextField text="" maxWidth="300" fx:id="text"/>
            <Button text="change password" onMouseClicked="#passwordChange"/>
            <Button text="back" onMouseClicked="#back"/>
        </VBox>
    </center>
</BorderPane>
